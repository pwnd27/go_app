scalar UUID

scalar Time

type User {
    id: UUID!
    username: String!
    full_name: String!
    email: String!
    password_changed_at: Time
    created_at: Time
    image: String
}

type Query {
    users: [User]!
}

input UserInput {
    username: String!
    password: String!
    full_name: String!
    email: String!
}

type Mutation {
    createUser(input: UserInput!): User!
}
